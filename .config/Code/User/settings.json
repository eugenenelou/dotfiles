{
  "editor.formatOnSave": true,
  "eslint.workingDirectories": ["front"],
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "Do MMMM YYYY, HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY/MM/DD",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code",
  "editor.bracketPairColorization.enabled": true,
  "[python]": {
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.semanticHighlighting.enabled": false,
    "editor.formatOnType": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tokenColorCustomizations": {
    // "textMateRules": [
    //   {
    //     "name": "comment",
    //     "scope": ["comment"],
    //     "settings": {
    //       "fontStyle": "italic"
    //     }
    //   },
    //   {
    //     "name": "Keyword Storage",
    //     "scope": ["keyword", "storage", "keyword.control"],
    //     "settings": {
    //       "fontStyle": "italic"
    //     }
    //   }
    // ],
    "variables": "#FFFFFF"
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#00A863"
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "diffEditor.renderSideBySide": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "emmet.optimizeStylesheetParsing": false,
  "emmet.showAbbreviationSuggestions": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.languageServer": "Pylance",
  "window.customMenuBarAltFocus": false,
  "prettier.useEditorConfig": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.insertMode": "replace",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "sql-formatter.dialect": "pl/sql",
  "python.testing.cwd": "backend",
  "json.maxItemsComputed": 25000,
  "python.defaultInterpreterPath": "",
  "window.titleBarStyle": "custom",
  "python.analysis.indexing": true,
  "git.mergeEditor": false,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "python.analysis.include": [".local", "backend"],
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "basic",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.views.commits.pullRequests.showForCommits": false,
  "gitlens.views.commits.pullRequests.showForBranches": false,
  "gitlens.views.commits.pullRequests.enabled": false,
  "gitlens.views.commitDetails.pullRequests.enabled": false,
  "gitlens.views.experimental.multiSelect.enabled": true,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.views.commitDetails.files.layout": "list",
  "codeium.enableCodeLens": false,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "gitlab.duoChat.enabled": false,
  "gitlab.aiAssistedCodeSuggestions.enabled": false,
  "[javascript][python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "helm-intellisense.customValueFileNames": ["values-prod.yaml", "values.yaml"],
  "scm.showOutgoingChanges": "always",
  "scm.showIncomingChanges": "never",
  "codeium.enableConfig": {
    "*": true
  },
  "black-formatter.importStrategy": "fromEnvironment"
}
