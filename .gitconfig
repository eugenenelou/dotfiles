[alias]
	st = status
	rbi = rebase -i
	plog = log --oneline --graph --decorate
	cim = commit -m
	cam = commit --amend
	co = checkout
	pf = push --force-with-lease
	br = branch
[core]
	excludesfile = ~/.gitignore
