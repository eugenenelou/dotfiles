[alias]
	st = status
	rb = rebase
	rbi = rebase -i
	rbc = rebase --continue
	plog = log --oneline --graph --decorate
	cim = commit -m
	cam = commit --amend
	co = checkout
	pf = push --force-with-lease
	br = branch
	wip = commit -m \"wip\" --no-verify
	pull = pull ff-only
	pop = reset HEAD~1
	fixup = commit --amend -C HEAD
	cfixup = commit -m \"chore: fixup!\"
	cp = cherry-pick
[core]
	excludesfile = ~/.gitignore
[user]
	name = Eugène Nélou
	email = eugene.nelou@gmail.com
[diff]
	colorMoved = zebra
[push]
	default = current
[rebase]
	gpgsign = true
	autosquash = true
[commit]
	gpgsign = true
